<div class="container mt-lg-3">
  <div class="row">
    <div class="col col-md-8 ">

      <div class="row justify-content-center text-center text-wrap fs-3 fw-bold">
        <div class="col-8">
          <%= @car_park.title %> reservations
        </div>
      </div>

      <% @car_park.reservations.includes(:user).current_reservations.each do |res|  %>
        <div>
          <%= res.user.name %> <%= res.user.email %>
          <%= res.start_date %> <%= res.end_date %>
          <%#= res.payment_status %> <%#= res.user.status %>
        </div>
      <% end %>
    </div>
  </div>
</div>
